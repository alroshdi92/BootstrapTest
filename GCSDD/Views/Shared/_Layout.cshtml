        <script>

            $("#DivStaffdata").display = 'none';
            $("#DivStaffdata").hide();

            function showstaff() {
                var staff = "";
                staff = $("#search_field").val();

                $.ajax({
                    type: 'POST',
                    dataType: 'json',
                    async: true,
                    cache: false,
                    contentType: 'application/json',
                    url: '.aspx/GetStaffdata',
                    data: JSON.stringify({ pMUNO: staff }),
                    success:
                        function (response) {
                            $('#lblName').text(response.d.Name);
                            $('#lblEmailAddress').text(response.d.Email);
                            $('#lblJobDescription').text(response.d.JobDescription);
                            $('#lblRefIndicator').text(response.d.RefIndicator);
                            $('#lblOfficeLoc').text(response.d.OfficeLoc);
                            $('#lblOfficePhone').text(response.d.OfficePhone);

                            $('#imgStaff').attr('src', '../assets/img/PersonImg.png');

                            if (response.d.Staff_Img) {

                                $('#imgStaff').attr('src', response.d.Staff_Img);
                            }

                            $("#DivStaffdata").show();
                            $("#keyboard").hide();
                        },
                    error: function () { }
                });
            }

            function Clearstaff() {

                $("#keyboard").show();
                $("#DivStaffdata").hide();
                $('#search_field').val('');
            }

            $(document).ready(function () {

                $.ajax({
                    type: 'POST',
                    dataType: 'json',
                    cache: false,
                    async: true,
                    contentType: 'application/json',
                    data: JSON.stringify({ PriorityID: 2 }),
                    url: '.aspx/GetNotifications',
                    success:
                        function (response) {
                            var div = $("#ulHighlights").empty();

                            var trHTML = '';

                            for (var i = 0; i < response.d.length; i++) {
                                trHTML += '<li>' + response.d[i].title + '</li>';
                            }

                            $('#ulHighlights').append(trHTML);

                            $('#divHighlights').breakingNews({
                                effect: 'typography',
                                themeColor: 'transparent',
                                height: 48,
                                fontSize: '32px',
                                delayTimer: 5000,
                            });
                        },
                    error: function () { }
                });


                $.ajax({
                    type: 'POST',
                    dataType: 'json',
                    cache: false,
                    async: true,
                    contentType: 'application/json',
                    data: JSON.stringify({ PriorityID: 3 }),
                    url: '.aspx/GetNotifications',
                    success:
                        function (response) {
                            var div = $("#ulnotifications").empty();

                            var trHTML = '';

                            for (var i = 0; i < response.d.length; i++) {
                                trHTML += '<li><span class="bn-seperator" style="background-image: url(../assets/img/scroll-news-logo.png); padding-left: 10px !important;"></span><a onclick="popupSelectedNotifications(' + response.d[i].notificationId + '); return false;"><b>' + response.d[i].title + '</b ></a ></li>';

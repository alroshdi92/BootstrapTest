                                trHTML2 += '<tr><td>' + response.d[i].date + '</td><td onclick="popupSelectedNotifications(' + response.d[i].notificationId + '); return false;">' + response.d[i].title + '</td><td>' + response.d[i].postedby + '</td></tr>';
                            }

                            trHTML2 += '</tbody></table>';
                            $('#divITupdates').append(trHTML2);

                            $('#divITUpdates').breakingNews({
                                height: 47,
                                fontSize: '16px',
                            });
                        },
                    error: function () { }
                });

                $(".mr-auto .nav-item").bind("click", function (event) {
                    event.preventDefault();
                    var clickedItem = $(this);
                    $(".mr-auto .nav-item").each(function () {
                        $(this).removeClass("active");
                    });
                    clickedItem.addClass("active");
                });

            });


            $('#myModalSelectedNotifications').on('shown.bs.modal', function () {

                var pNotificationId = "";
                pNotificationId = document.getElementById('hdnNotificationId').value;

                $.ajax({
                    url: '.aspx/GetNotificationsByID',
                    dataType: "json",
                    type: "POST",
                    cache: false,
                    async: true,
                    contentType: "application/json",
                    data: JSON.stringify({ NotificationId: pNotificationId }),
                    success: function (response) {
                        $('#spnNotificationHeader').text(response.d.title);
                        $('#spnNotificationDate').text(response.d.date);
                        $('#pNotificationbody').text(response.d.content);
                    },
                    error: function () { }
                });
            });

        </script>
    </form>
</body>
</html>


            function dateToYMD(date) {
                var strArray = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
                //var strWeekArray = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];
                var d = date.getDate();
                var m = strArray[date.getMonth()];
                var y = date.getFullYear();
                //var day = strWeekArray[date.getDay()];
                //return day + ', ' + (d <= 9 ? '0' + d : d) + ' ' + m + ' ' + y;
                return (d <= 9 ? '0' + d : d) + ' ' + m + ' ' + y;
            };

            var datestring = dateToYMD(new Date());

            document.getElementById("currentdate").innerHTML = datestring;

            function getTodayDate(divElementId, withTime) {

                var todayDtTm = new Date();
                var dd = String(todayDtTm.getDate()).padStart(2, '0');
                var mm = String(todayDtTm.getMonth() + 1).padStart(2, '0'); //January is 0!
                var yyyy = todayDtTm.getFullYear();

                var today = dd + '/' + mm + '/' + yyyy;

                if (withTime) {
                    var curHour = todayDtTm.getHours() > 12 ? (todayDtTm.getHours() - 12) < 10 ? '0' + (todayDtTm.getHours() - 12) : (todayDtTm.getHours() - 12) : (todayDtTm.getHours() < 10 ? '0' + todayDtTm.getHours() : todayDtTm.getHours());
                    var curMinute = todayDtTm.getMinutes() < 10 ? '0' + todayDtTm.getMinutes() : todayDtTm.getMinutes();
                    var curMeridiem = todayDtTm.getHours() > 12 ? 'PM' : 'AM';

                    today += ' ' + curHour + ':' + curMinute + ' ' + curMeridiem;
                }

                document.getElementById(divElementId).innerHTML = today;
            };

            function startTimer(duration, display) {
                var timer = duration, minutes, seconds;
                setInterval(function () {
                    minutes = parseInt(timer / 60, 10);
                    seconds = parseInt(timer % 60, 10);

                    minutes = minutes < 10 ? "0" + minutes : minutes;
                    seconds = seconds < 10 ? "0" + seconds : seconds;

                    display.text(minutes + ":" + seconds);

                    if (--timer < 0) {
                        timer = duration;
                    }
                }, 1000);
            }

